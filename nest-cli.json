{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/crypto-wallet-system/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/crypto-wallet-system/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/crypto-wallet-system",
  "projects": {
    "crypto-wallet-system": {
      "type": "application",
      "root": "apps/crypto-wallet-system",
      "entryFile": "main",
      "sourceRoot": "apps/crypto-wallet-system/src",
      "compilerOptions": {
        "tsConfigPath": "apps/crypto-wallet-system/tsconfig.app.json"
      }
    },
    "wallet": {
      "type": "application",
      "root": "apps/wallet",
      "entryFile": "main",
      "sourceRoot": "apps/wallet/src",
      "compilerOptions": {
        "tsConfigPath": "apps/wallet/tsconfig.app.json"
      }
    },
    "rate": {
      "type": "application",
      "root": "apps/rate",
      "entryFile": "main",
      "sourceRoot": "apps/rate/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rate/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    }
  }
}